<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <configSections>
        <section name="Throttling" type="Dxw.Throttling.Core.Configuration.ThrottleConfiguration, Dxw.Throttling.Core"/>
        <section name="Throttling1" type="Dxw.Throttling.Core.Configuration.ThrottleConfiguration1, Dxw.Throttling.Core"/>
    </configSections>

    <Throttling>
        <quota maxHits="2" perSeconds="3" type="Dxw.Throttling.Core.Keyer.ConstantKeyer, Dxw.Throttling.Core"/>
    </Throttling>

    <Throttling1>
        <storages>
            <storage type="Dxw.Throttling.Core.Storage.LocalMemoryStorage, Dxw.Throttling.Core" name="local" />
        </storages>
        <rules>
            <rule type="Dxw.Throttling.Core.Rules.StorageKeyerProcessorRule">
                <storage name="local" />
                <keyer type="Dxw.Throttling.Core.Keyer.ConstantKeyer" />
                <processor type="Dxw.Throttling.Core.EventProcessor.RequestCountPerPeriodProcessor" count="3" period="00:00:10" />
            </rule>
        </rules>
    </Throttling1>

    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6" />
    </startup>
</configuration>