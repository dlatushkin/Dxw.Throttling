<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <configSections>
        <!--<section name="throttling" type="Dxw.Throttling.Core.Configuration.ConfigurationSectionHandler, Dxw.Throttling.Core" />-->

        <section name="throttling" type="Dxw.Throttling.Core.Configuration.ConfigurationSectionHandler`1[[Dxw.Throttling.Core.Rules.PassBlockVerdict, Dxw.Throttling.Core, Version=1.0.0.8, Culture=neutral, PublicKeyToken=null]], Dxw.Throttling.Core, Version=1.0.0.8, Culture=neutral, PublicKeyToken=null" />
        
    </configSections>

    <throttling>
        <storages>
            <storage type="Dxw.Throttling.Redis.Storages.RedisStorage, Dxw.Throttling.Redis" name="redis" connectionString ="localhost" />
        </storages>
        <rules>
            <rule type="Dxw.Throttling.Core.Rules.StorageKeyerProcessorPassBlockRule" name="singleRedis">
                <storage name="redis" />
                <keyer type="Dxw.Throttling.Core.Keyers.ConstantKeyer" />
                <processor type="Dxw.Throttling.Redis.Processors.RequestCountPerPeriodProcessorBlockPass, Dxw.Throttling.Redis"
                           count="2" period="00:00:01" />
            </rule>
        </rules>
    </throttling>
</configuration>